<form id="createProjectForm" method="post" class="edit-project-form" action="/p/create-project/blog" enctype="multipart/form-data">
   <div class="form__container">

      <div class="modalBtnMeta project_meta_btn">
         <img src="/icons/settings-black.png" class="main-topnav__icon" />
      </div>


      {{! Project Title }}
      <input class="form__input project_title_input" type="text" placeholder="Enter a title" name="project_title" value="{{project.project_title}}" autocapitalize="none" />


      {{! Project Notes }}
      <div id="editor" name="editor"  placeholder="Hi {{user.username}}, you have 1000 words. Have fun!"></div>
      <input id="projectNotes" name="project_notes" type="hidden">


      <input type="hidden" name="user_id" value="{{user._id}}">
      <p id="wordCount"></p>

      <input id="checkbox" type="hidden" name="is_private" value="false">
      <input type="hidden" name="admin" value="{{user.username}}">
      <input type="hidden" name="id" value="{{user._id}}">
      <input type="hidden" name="_x_" value="$2a$10$BJn0b4OzBarDigcTUl0kejbffkejbfvxSI.JrlLKeSsOi8oKqBAK" />
      <input type="hidden" name="_c_" value="{{csrf_web_token}}" />
      <input type="hidden" name="orange_blossom" value="" />
      <input type="hidden" id="img_count" name="img_indices" value="" />
      <div class="create_btns">
         <button class="form__sign-up-button project_submit edit-project__form-btn" name="submit" type="submit">Create</button>
         <button class="form__sign-up-button save_as_draft_btn draft_btn project_submit edit-project__form-btn" name="submit" type="submit">Save as Draft</button>
      </div>
  </div>

  {{! Project Meta Modal }}
  <div class="popModalMeta modalBackground">
     <div class="modal-content">

        {{#if error_msg}}
        <div class="error__msg-3">
           <p>{{error_msg}}</p>
           <p class="error__exit">&times;</p>
        </div>
        {{/if}}

        {{#if errors}}
           {{#each errors}}
              <div class="error__msg-3">
                 <p>{{msg}}</p>
                 <p class="error__exit">&times;</p>
              </div>
           {{/each}}
        {{/if}}

        {{! Public or Private }}
        <div class="checkbox__wrapper">
           <label class="check__container">
              <input id="isPrivateCheck" class="form__input-checkbox" type="checkbox">
              <span class="checkmark">Private Project</span>
           </label>
           <label class="check__container">
              <input id="isPublicCheck" class="form__input-checkbox checked" type="checkbox" checked>
              <span class="checkmark">Public Project</span>
           </label>
        </div>
        <br />
        <p class="form__input-small" id="changeText">A public project can be viewed by anyone</p>

        {{! Project Image }}
        <label for="project_image" class="form__label">Add Project Image <span class="form_span_descriptor">* Required *</span></label>
        <input id="projectImage" class="form__input" type="file" name="project_image" value="{{project.project_image}}" />


        {{! Thumbnail Image }}
        <label for="thumbnail_image" class="form__label">Add Thumbnail Image <span class="form_span_descriptor">( Optional )</span></label>
        <input id="thumbnailImage" class="form__input" type="file" name="thumbnail_image" value="{{project.thumbnail_image}}"/>


         {{! Project Video }}
         <label for="project_video" class="form__label">Add Video <span class="form_span_descriptor">( Thumbnail image is required )</span></label>
         <input class="form__input" type="file" name="project_video" value="{{project.project_video}}" />
         <small style="font-size: 12px; display: block; font-style: italic; color: #999; margin: -25px 0 25px 0;">Video is still in progress. May experience some issues.</small>


        {{! Post To }}
        <label for="post_to" class="form__label" style="margin-top: 25px;">Post To <span class="form_span_descriptor">( Optional )</span></label>
        <select data-placeholder="Post to..." class="chosen-select form__input" name="post_to">
           <option value=""></option>
           <option value="followers">All Followers</option>
           {{#each user.collections}}
           <option value="{{this.collection_id}}">Your Collection "{{this.collection_name}}"</option>
           {{/each}}
           {{#each user.groups}}
           <option value="{{this.group_id}}">Your Group "{{this.group_name}}"</option>
           {{/each}}
        </select>


        {{! Project Categories }}
        <label for="project_categories" class="form__label" style="margin-top: 25px;">Add Project Categories <span class="form_span_descriptor">( Optional )</span></label>
        <select data-placeholder="Add categories..." multiple class="chosen-select form__input" name="project_categories[]">
           <option value=""></option>
           <option>Art</option>
           <option>Beauty & Fashion</option>
           <option>Business</option>
           <option>Family & Society</option>
           <option>Food & Drink</option>
           <option>Health & Wellness</option>
           <option>Home & Garden</option>
           <option>Science & Technology</option>
           <option>Sports & Fitness</option>
           <option>Travel</option>
        </select>


        {{! Project URL }}
        <label for="project_url" class="form__label" style="margin-top: 25px;">Add a link <span class="form_span_descriptor">( Optional )</span></label>
        <input class="form__input" type="text" placeholder="Enter a link" name="project_url" value="{{project.project_url}}" />


        {{! Project Description }}
        <label for="project_description" class="form__label" style="margin-top: 25px;">Write a Description <span class="form_span_descriptor">( Optional )</span></label>
        <textarea class="project-description__input" type="text" name="project_description" value="{{project.project_description}}">{{project.project_description}}</textarea>


        <div class="modal-footer">
           <span class="closeModalMeta btn__primary">Done</span>
        </div>

     </div>

  </div>
</form>
<p class="go-back back-btn" style="margin-top: 50px;">Cancel</p>

