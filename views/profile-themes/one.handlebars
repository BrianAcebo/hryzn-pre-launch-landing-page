<div class="profile_page" style="cursor: url(/cursors/{{profile.profile_cursor}}.cur),auto;">

<!-- Navigation bar with social media icons -->
<div class="template_one-bar profile_main_accent_color">
   {{#if profile.website}}
   <a target="_blank" href="https://{{profile.website}}" class="template_one-bar-item template_one-button"><i class="fa fa-desktop"></i></a>
   {{/if}}
   {{#if profile.youtube}}
   <a target="_blank" href="https://www.youtube.com/channel/{{profile.youtube}}" class="template_one-bar-item template_one-button"><i class="fa fa-youtube-play"></i></a>
   {{/if}}
   {{#if profile.twitter}}
   <a target="_blank" href="https://twitter.com/{{profile.twitter}}" class="template_one-bar-item template_one-button"><i class="fa fa-twitter"></i></a>
   {{/if}}
   {{#if profile.instagram}}
   <a target="_blank" href="https://www.instagram.com/{{profile.instagram}}" class="template_one-bar-item template_one-button"><i class="fa fa-instagram"></i></a>
   {{/if}}
   {{#if profile.facebook}}
   <a target="_blank" href="https://www.facebook.com/{{profile.facebook}}" class="template_one-bar-item template_one-button"><i class="fa fa-facebook"></i></a>
   {{/if}}
</div>


<div class="template_one-content">

   <!-- Image header -->
   <div class="profile_bg_container">
      {{#if profile.backgroundimage}}
      <img class="profile_bg" src="https://ik.imagekit.io/w07am55tja/{{profile.backgroundimage}}?tr=w-1920" />
      {{else}}
      <img class="profile_bg" src="/img/site/hryzn-placeholder-01.jpg" />
      {{/if}}

      <div class="template_one-display-left template_one-padding">
         <hr style="width: 75px;height: 2px;background: #fff;margin: 0 15px 7px 0;border: none;display: inline-block;">
         <h3 style="display: inline-block" class="template_one-text-white profile_secondary_font">{{#if profile.firstname}}{{profile.firstname}} {{profile.lastname}}{{else}}{{/if}}</h3>
         <h1 class="template_one-jumbo template_one-text-white template_one-text-caps profile_main_font"><strong>{{profile.username}}</strong></h1>
         {{#if viewing_own_profile}}
         <div class="template_one_btn_wrapper">
            <a>
               <div class="msg_btn profile_btns_rounding profile_main_accent_color profile_secondary_font">You're Amazing</div>
            </a>
            <a>
               <div id="settingsBtn" class="profile__follow profile_btn profile_btns_rounding profile_secondary_accent_color profile_secondary_font">Settings</div>
            </a>
         </div>
         {{else}}
         <div class="template_one_btn_wrapper">

           {{#if profile.is_private_profile}}
             {{#if user_follows_profile}}
             <a href="/messages/{{profile.username}}">
                <div class="msg_btn profile_btns_rounding profile_main_accent_color profile_secondary_font">Message</div>
             </a>
            {{else}}
            <a>
               <div class="msg_btn profile_btns_rounding profile_main_accent_color profile_secondary_font">Message</div>
            </a>
             {{/if}}
           {{else}}
            <a href="/messages/{{profile.username}}">
               <div class="msg_btn profile_btns_rounding profile_main_accent_color profile_secondary_font">Message</div>
            </a>
            {{/if}}

            {{#if user_follows_profile}}
            <a class="modalBtn">
               <div class="profile__unfollow profile_btn profile_btns_rounding profile_secondary_accent_color profile_secondary_font">Unfollow</div>
            </a>

            {{else}}

              {{#if viewer_has_pending_request}}

              <a>
                 <div class="profile__follow profile_btn profile_btns_rounding profile_secondary_accent_color profile_secondary_font">Requested</div>
              </a>

              {{else}}

              {{#if can_view_adult_content}}
              <form method="post" action="{{#if on_wildcard_profile}}https://www.myhryzn.com{{/if}}/profile/follow/{{profile._id}}">
                 <input type="hidden" name="user_username" value="{{user.username}}">
                 <input type="hidden" name="profile_username" value="{{profile.username}}">
                 <button class="profile__follow profile_btn profile_btns_rounding profile_secondary_accent_color profile_secondary_font" name="submit" type="submit">
                   Follow
                   {{#if premium_creator_account}}
                   {{#if profile.completed_onboard_payouts}}
                   {{#ifEquals profile.premium_creator_account '3'}}
                   {{#if profile.creator_subscription.is_active}}
                   <span class="creator_sub_btn"> for {{profile.creator_subscription.current_price}}</span>
                   {{/if}}
                   {{/ifEquals}}
                   {{/if}}
                   {{/if}}
                 </button>
              </form>
              {{/if}}

              {{/if}}
            {{/if}}
         </div>
         {{/if}}

         {{#if premium_creator_account}}
         {{#if profile.completed_onboard_payouts}}
         {{#ifEquals profile.premium_creator_account '0'}}
         {{else}}
         <a class="payments_modal_btn">
            <div class="profile_btn profile_btns_rounding profile_secondary_accent_color profile_secondary_font send_tip_btn">Send A Tip</div>
         </a>
         {{/ifEquals}}
         {{/if}}
         {{/if}}

      </div>

      <div class="bg_overlay"></div>
   </div>

