{{! Collections Modal}}
<div class="groupModal groupModalBackground {{#if group_error}}groupError{{/if}}">
   <div class="modal-content collection_form_modal">

   <form method="post" class="edit-project-form" action="{{#if on_wildcard_profile}}https://www.myhryzn.com{{/if}}/create-collection">
      {{! Public or Private }}
      <div class="checkbox__wrapper">
         <label class="check__container">
            <input id="isPrivateCheck" class="form__input-checkbox" type="checkbox">
            <span class="checkmark">Private Collection</span>
         </label>
         <label class="check__container">
            <input id="isPublicCheck" class="form__input-checkbox checked" type="checkbox" checked>
            <span class="checkmark">Public Collection</span>
         </label>
      </div>
      <br />
      <input id="checkbox" type="hidden" name="is_private" value="false">

      {{! Collection Name }}
      <label for="the_collection_name" class="form__label" style="margin-top: 25px;">Collection Name <span style="color: #999; font-size: 10px; margin-left: 8px;">* Required *</span></label>
      <input class="form__input" type="text" placeholder="Let's name the collection" name="the_collection_name" value="{{the_collection_name}}" required />


      {{! Collection Categories }}
      <label for="collection_categories" class="form__label" style="margin-top: 25px;">Add Categories <span style="color: #999; font-size: 10px; margin-left: 8px;">( Optional )</span></label>
      <select data-placeholder="Add categories..." multiple class="chosen-select form__input" name="collection_categories[]">
         <option value=""></option>
         <option>Art</option>
         <option>Beauty & Fashion</option>
         <option>Business</option>
         <option>Family & Society</option>
         <option>Food & Drink</option>
         <option>Health & Wellness</option>
         <option>Home & Garden</option>
         <option>Science & Technology</option>
         <option>Sports & Fitness</option>
         <option>Travel</option>
      </select>

      {{#if followers}}
      <label for="collection_categories" class="form__label" style="margin-top: 25px;">By choosing who can see this collection, it will be private<span style="color: #999; font-size: 10px; margin-left: 8px;">( Optional )</span></label>
      <section class="follower__profiles-section">
         {{#each followers}}
         <label class="checkbox-img" >
            <input type="checkbox" name="collection_followers[]" value="{{_id}}">
            <a class="follower__profile">
               <div class="follower__profile-img-container">
                  <img class="profile__img" {{#if this.profileimage}}src="https://ik.imagekit.io/w07am55tja/{{this.profileimage}}?tr=w-500"{{else}}src="/img/site/hryzn-placeholder-01.jpg"{{/if}} />
               </div>
               @{{this.username}}
            </a>
         </label>
         {{/each}}
      </section>
      {{/if}}

      {{#if projects}}
      {{! Collection Projects }}
      <label for="collection_categories" class="form__label" style="margin-top: 25px;">Add Projects <span style="color: #999; font-size: 10px; margin-left: 8px;">( Optional )</span></label>
      <div class="collection_form_projects_container create_collection_projects">
      {{#each projects}}
         <div class="collection_form_projects">
            <label class="checkbox-img" >
               <input type="checkbox" name="collection_projects[]" value="{{_id}}">
               {{#if is_micro_post}}
