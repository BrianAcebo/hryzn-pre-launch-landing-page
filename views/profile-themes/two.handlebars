<div class="profile_page" style="cursor: url(/cursors/{{profile.profile_cursor}}.cur),auto;">

<!-- Sidebar/menu -->
<nav class="template_two-sidebar template_two-collapse template_two-top template_two-large template_two-padding profile_main_accent_color mobile_side_nav" style="width:300px;font-weight:bold;" id="mySidebar"><br>
  <a href="javascript:void(0)" onclick="w3_close()" class="template_two-button template_two-hide-large template_two-display-topleft" style="width:75px;font-size:35px">&times;</a>
  <div class="template_two-container template_two-padding-64">
     {{#if profile.profileimage}}
     <img class="template_two-profile-img profile_btns_rounding template_two_desktop_margin_top" src="https://s3.amazonaws.com/hryzn-app-static-assets/{{profile.profileimage}}" />
     {{/if}}
    <h3>
      <p class="profile_secondary_font profile_main_accent_color">{{profile.firstname}} {{profile.lastname}}</p>
      <b class="profile_main_font profile_main_accent_color">@{{profile.username}}</b>
   </h3>
   <br />
   {{#if profile.bio}}
   <p class="profile_secondary_font profile_main_accent_color">{{profile.bio}}</p>
   <br />
   {{/if}}
   <div class="template_two_followers__following">
      <a class="profile_secondary_font profile_main_accent_color" href="/profile/{{profile.username}}/followers"><strong>{{amount_of_followers}}</strong> followers</a>
      <a class="profile_secondary_font profile_main_accent_color" href="/profile/{{profile.username}}/following"><strong>{{amount_of_following}}</strong> following</a>
   </div>
  </div>
</nav>

<!-- Top menu on small screens -->
<header class="template_two-mobile-nav">
